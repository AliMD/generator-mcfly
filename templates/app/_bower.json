  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "1.3.15",
    "angular-mocks": "1.3.15",
    "angular-animate": "1.3.15",
    "angular-sanitize": "1.3.15",
    "angular-resource": "1.3.15",
